{
  "name": "demo-emailing-system",
  "version": "1.0.0",
  "description": "NextJS & Typescript with Tailwind CSS, running on Node backend",
  "scripts": {
    "dev": "next",
    "build2": "next build",
    "start2": "next start",
    "build": "rm -rf ./build && next build && mkdir build && babel ./src/server --out-dir build",
    "start1": "npm run build && node ./build/index.js",
    "start": "node ./build/index.js",
    "ts-start": "ts-node ./build/index.js",
    "serve:dev": "nodemon --exec babel-node src/server/index.js",
    "test": "npm run test:format && npm run test:lint && npm run test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:format": "prettier --check .",
    "test:tsc": "tsc --noEmit",
    "test:lint": "eslint './src/**/*' --ext .tsx,.ts",
    "heroku-postbuild-disable": "npm run undo:prod && npm run migrate:prod",
    "start:dev": "nodemon --exec babel-node -- src/server/index.js",
    "migrate:dev": "sequelize db:migrate",
    "migrate:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate && sequelize db:seed:all",
    "undo:prod": "cross-env NODE_ENV=production sequelize db:migrate:undo:all",
    "undo:dev": "sequelize db:migrate:undo:all",
    "undo:test": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "seed:dev": "sequelize db:seed:all",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all",
    "undo:seed": "NODE_ENV=development npx sequelize-cli db:seed:undo:all"
  },
  "husky-disabled": {
    "hooks": {
      "pre-commit": "npm run test:tsc && npm run test:lint",
      "pre-push": "npm run test"
    }
  },
  "author": "Rasheed",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@sendgrid/mail": "^7.2.6",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "formik": "^2.1.5",
    "framer-motion": "^2.6.15",
    "match-sorter": "^4.2.1",
    "moment": "^2.28.0",
    "mysql2": "^2.2.5",
    "namor": "^2.0.2",
    "next": "^9.4.4",
    "nodemailer": "^6.4.11",
    "nprogress": "^0.2.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-loadingmask": "^4.0.6",
    "react-switch": "^5.0.1",
    "react-table": "^7.5.1",
    "react-toastify": "^6.0.8",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "styled-components": "^5.2.0",
    "tailwindcss-dir": "^4.0.0",
    "tailwindcss-elevation": "^0.3.5",
    "ts-node": "^9.0.0",
    "url": "^0.11.0",
    "use-dark-mode": "^2.3.1",
    "uuid": "^8.3.0",
    "xlsx": "^0.16.7",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@mdx-js/loader": "^1.6.5",
    "@next/mdx": "^9.5.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-transform-stub": "^2.0.0",
    "nodemon": "^2.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "tailwindcss": "^1.4.6",
    "tailwindcss-dark-mode": "^1.1.4",
    "tailwindcss-spinner": "^1.2.0",
    "typescript": "^4.0.3"
  },
  "proxy": "http://localhost:3003"
}
